{
  "automergeType": "branch",
  "ignoreTests": true,
  "packageRules": [
    {
      "matchDatasources": ["galaxy", "galaxy-collection"],
      "matchUpdateTypes": ["minor", "patch", "digest"],
      "automerge": true
    },
    {
      "matchDatasources": ["docker"],
      "matchUpdateTypes": ["patch", "digest"],
      "automerge": true
    },
    {
      "matchDatasources": ["docker"],
      "matchPackageNames": ["eqalpha/keydb", "rabbitmq"],
      "automerge": false
    }
  ]
}
